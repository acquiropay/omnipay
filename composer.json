{
  "name": "acquiropay/omnipay-acquiropay",
  "description": "AcquiroPay driver for the Omnipay payment processing library",
  "keywords": [
    "omnipay",
    "acquiropay"
  ],
  "homepage": "https://github.com/acquiropay/omnipay-acquiropay",
  "license": "MIT",
  "require": {
    "php": "^5.6|^7",
    "omnipay/common": "~3.0"
  },
  "require-dev": {
    "omnipay/tests": "~3.0",
    "scrutinizer/ocular": "~1.1",
    "larapack/dd": "^1.1",
    "php-http/guzzle6-adapter": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Omnipay\\AcquiroPay\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Omnipay\\AcquiroPay\\Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1-dev"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
